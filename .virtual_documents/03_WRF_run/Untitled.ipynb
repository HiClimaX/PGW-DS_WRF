new_lat_da = xr.DataArray(lat, dims='lat')
new_lon_da = xr.DataArray(lon, dims='lon')



new_lat_da


# Interpolate the dataset
interpolated_ds = ds.interp(lat=new_lat_da, lon=new_lon_da, method='linear')

